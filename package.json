{
  "name": "vscode-open-in-code",
  "displayName": "Open in Code",
  "description": "Switch between Code and Code Insiders with ease.",
  "icon": "resources/logo-128x128.png",
  "version": "1.1.0",
  "license": "MIT",
  "main": "out/src/extension.js",
  "publisher": "fabiospampinato",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "openInCode.open",
        "title": "Open in Code"
      },
      {
        "command": "openInCode.openInsiders",
        "title": "Open in Code Insiders"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "openInCode.open",
          "when": "isInsiders"
        },
        {
          "command": "openInCode.openInsiders",
          "when": "!isInsiders"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "publish": "vsce publish && npm publish",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/vscode-open-in-code/issues"
  },
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/vscode-open-in-code.git"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "keywords": [
    "vscode",
    "vsc",
    "extension",
    "open in",
    "code"
  ],
  "categories": [
    "Other"
  ],
  "dependencies": {
    "absolute": "0.0.1",
    "lodash": "^4.17.4",
    "open": "0.0.5"
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "tslint": "^4.4.2",
    "typescript": "~2.2.0",
    "vscode": "^1.1.4"
  }
}
